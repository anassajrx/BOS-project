        "\t\t\testApprouveParPerm: $data.projetDeLoi.estApprouveParPerm !== undefined ? $data.projetDeLoi.estApprouveParPerm : null,",
        "\t\t\testApprouveParRepr: $data.projetDeLoi.estApprouveParRepr !== undefined ? $data.projetDeLoi.estApprouveParRepr : null",
