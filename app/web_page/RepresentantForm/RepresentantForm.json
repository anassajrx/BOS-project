      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.projetDeLoi ){",
        "\treturn {",
        "\t\t//map projetDeLoi variable to expected task contract input",
        "\t\tprojetDeLoiInput: {",
        "\t\t\tministre: $data.projetDeLoi.ministre !== undefined ? $data.projetDeLoi.ministre : null,",
        "\t\t\tdateDeDepot: $data.projetDeLoi.dateDeDepot !== undefined ? $data.projetDeLoi.dateDeDepot : null,",
        "\t\t\tdescription: $data.projetDeLoi.description !== undefined ? $data.projetDeLoi.description : null,",
        "\t\t\testApprouveParRepr: $data.projetDeLoi.estApprouveParRepr !== undefined ? $data.projetDeLoi.estApprouveParRepr : null",
